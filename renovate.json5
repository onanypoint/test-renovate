{
  $schema: 'https://docs.renovatebot.com/renovate-schema.json',
  branchPrefix: 'chore-renovate-',
  constraintsFiltering: 'strict',
  rangeStrategy: "update-lockfile",
  separateMajorMinor: true,
  lockFileMaintenance: {
    enabled: true,
